# The port n8n will run on. Default is 5678.
N8N_PORT=5678

# The host domain n8n will be accessible on.
N8N_HOST=localhost

# Protocol to use. Can be http or https.
N8N_PROTOCOL=http

# Environment for Node.js. Should be 'production'.
NODE_ENV=production

# Allow connections from any origin for the frontend UI.
N8N_CORS_ALLOW_ORIGIN=*

# Set your desired username and a strong password for logging into n8n.
N8N_BASIC_AUTH_USER=your_username
N8N_BASIC_AUTH_PASSWORD=your_strong_password